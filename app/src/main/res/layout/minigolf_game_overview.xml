<?xml version="1.0" encoding="utf-8"?>
<dan.dit.gameMemo.util.RelativeLayoutCheckable xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayoutCheckable1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/minigolf_list_selector"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/minigolf_overview_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

        <TextView
            android:id="@+id/time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <CheckBox
            android:id="@+id/selected_checker"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:focusable="false"
            android:focusableInTouchMode="false" />
    </LinearLayout>

    <TextView
        android:id="@+id/minigolf_players"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@id/minigolf_overview_header" />

    <TextView
        android:id="@+id/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/minigolf_players"
        android:drawableLeft="@drawable/location"
        android:text="TextView" />

    <TextView
        android:id="@+id/winner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/location"
        android:drawableLeft="@drawable/winner"
        android:text="TextView" />

</dan.dit.gameMemo.util.RelativeLayoutCheckable>